{
    "policy": [
        {
            "Action": [
                "ec2:CreateTransitGatewayMulticastDomain"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:ec2:{region}:{account_id}:transit-gateway/{transit_gateway_id}",
                "arn:aws:ec2:{region}:{account_id}:transit-gateway-multicast-domain/*"
            ]
        },
        {
            "Action": [
                "ec2:CreateTags"
            ],
            "Effect": "Allow",
            "Resource": [
                "*"
            ]
        }
    ]
}
