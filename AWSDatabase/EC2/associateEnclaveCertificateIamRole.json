{
    "policy": [
        {
            "Action": [
                "ec2:AssociateEnclaveCertificateIamRole"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:acm:{region}:{account_id}:certificate/{certificate_id}",
                "arn:aws:iam::{account_id}:role_name/{role_name}"
            ]
        }
    ]
}