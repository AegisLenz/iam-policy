{
    "policy": [
        {
            "Action": [
                "ec2:CreateNatGateway"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:ec2:{region}:{account_id}:allocation/{allocation_id}",
                "arn:aws:ec2:{region}:{account_id}:subnet/{subnet_id}",
                "arn:aws:ec2:{region}:{account_id}:natgateway/*"
            ]
        }
    ]
}