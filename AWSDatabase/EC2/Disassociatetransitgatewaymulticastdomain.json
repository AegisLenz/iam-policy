{
    "policy": [
        {
            "Action": [
                "ec2:DisassociateTransitGatewayMulticastDomain"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:ec2:{region}:{account_id}:subnet/{subnet_id}",
                "arn:aws:ec2:{region}:{account_id}:attachment/{attachment_id}",
                "arn:aws:ec2:{region}:{account_id}:transit-gateway-multicast-domain/{transit_gateway_multicast_domain_id}"
            ]
        }
    ]
}